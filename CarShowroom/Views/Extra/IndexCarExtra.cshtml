@model IEnumerable<CarShowroom.Entities.CarExtra>

@{
    ViewData["Title"] = "IndexCarExtra";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>IndexCarExtra</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Car)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Extra)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Car.CarId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Extra.ExtraId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.CarExtraId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.CarExtraId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.CarExtraId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
